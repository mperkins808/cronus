// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

model Datasource {
  id         String  @id @default(cuid())
  name       String?
  path       String
  authheader String?
  iv         String?
  created_at Float?
}

model DSQuery {
  id              String  @id @default(cuid())
  name            String?
  datasource_id   String
  raw_query       String
  step            String
  time            String
  datasource_type String
  cronus_label    String?
  created_at      Float?
}

model Users {
  username    String  @id
  displayname String?
  password    String
  role        String
  created_at  Float?
}

model Session {
  id       String @id
  username String
  expiry   Int
}

datasource db {
  provider = "DB_PROVIDER_PLACEHOLDER"
  url      = "DB_URL_PLACEHOLDER"
}
